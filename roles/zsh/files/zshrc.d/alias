alias ls="ls --color --quoting-style=literal"
alias ll="ls -lhs"
alias la="ls -a"
alias lla="ll -a"
alias df="df -h"
alias du="du -h"
alias free="free -h"
alias vi="vim"
alias nano="nano -w"
alias grep="grep --color=auto"
alias less="less -cR"
alias lv="less"
alias su="sudo -sE"
alias libreoffice="LANG=ja_JP.UTF-8 libreoffice"
